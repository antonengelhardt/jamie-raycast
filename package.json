{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "jamie",
  "title": "jamie",
  "description": "AI-powered meeting notes",
  "icon": "command-icon.png",
  "author": "antonengelhardt",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "start-meeting",
      "title": "Start Meeting",
      "subtitle": "Start a new meeting",
      "description": "jamie will listen to your meeting and take notes",
      "mode": "view"
    },
    {
      "name": "stop-meeting",
      "title": "Stop Meeting",
      "subtitle": "Stop current meeting",
      "description": "Stop recording the current meeting and generate notes",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}